@model Shopping.Data.Entities.City

@{
    ViewData["Title"] = "DeleteCity";
}

<h1>Borrar Cuidad</h1>

<h3>¿Está seguro de eliminar esta Cuidad?</h3>
<div>
    
    <partial name="_CityDetails" />
    
    <form asp-action="DeleteCity">
        <input type="hidden" asp-for="Id"/>
        <input type="submit" value="Borrar" class="btn btn-outline-danger" /> |
        <a asp-action="DetailsState" asp-route-id="@Model?.State.Id" class="btn btn-outline-primary">Volver</a>
    </form>
</div>
