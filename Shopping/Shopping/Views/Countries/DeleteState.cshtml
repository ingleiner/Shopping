@model Shopping.Data.Entities.State

@{
    ViewData["Title"] = "DeleteState";
}

<h1>Borrar</h1>

<h3>¿Está seguro de eliminar este Departamento/Estado?</h3>
<div>
    
    <partial name="_StateDetails" />
    
    <form asp-action="DeleteState">
        <input type="hidden" asp-for="Id"/>
        <input type="submit" value="Borrar" class="btn btn-outline-danger" /> |
        <a asp-action="Details" asp-route-id="@Model?.Country.Id" class="btn btn-outline-primary">Volver</a>
    </form>
</div>
